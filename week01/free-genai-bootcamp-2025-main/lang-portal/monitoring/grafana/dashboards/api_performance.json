{
  "title": "API Performance Overview",
  "uid": "api-performance",
  "version": 1,
  "panels": [
    {
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(handler_request_count{job=\"lang-portal\"}[5m])",
          "legendFormat": "{{endpoint}}"
        }
      ]
    },
    {
      "title": "Response Time Percentiles",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(handler_request_duration_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(handler_request_duration_bucket[5m]))",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(handler_error_count[5m])",
          "legendFormat": "{{type}}"
        }
      ]
    },
    {
      "title": "Cache Performance",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(cache_get_hit_total[5m]) / (rate(cache_get_hit_total[5m]) + rate(cache_get_miss_total[5m]))",
          "legendFormat": "Hit Rate"
        }
      ]
    },
    {
      "title": "Endpoint Status",
      "type": "status-history",
      "gridPos": {"x": 0, "y": 16, "w": 24, "h": 6},
      "targets": [
        {
          "expr": "rate(handler_request_count{code!=\"200\"}[5m]) > 0",
          "legendFormat": "{{endpoint}}"
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "endpoint",
        "type": "query",
        "query": "label_values(handler_request_count, endpoint)",
        "refresh": 2
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  }
} 